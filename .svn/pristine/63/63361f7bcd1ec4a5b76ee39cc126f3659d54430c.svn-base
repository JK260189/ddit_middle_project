package kr.or.ddit.member.vo;

public class MemberVO {
	
	private int m_code;
	private String m_id;
	private String m_pass;
	private String m_name;
	private String m_email;
	private String m_api;
	private String m_emailcheck;
	private String m_addr;
	
	private String r_title;
	private String r_content;
	
	
	
	public String getR_title() {
		return r_title;
	}



	public void setR_title(String r_title) {
		this.r_title = r_title;
	}



	public String getR_content() {
		return r_content;
	}



	public void setR_content(String r_content) {
		this.r_content = r_content;
	}



	public MemberVO() {
		
	}



	public MemberVO(String m_id, String m_pass, String m_name, String m_email, String m_addr) {
		this.m_id = m_id;
		this.m_pass = m_pass;
		this.m_name = m_name;
		this.m_email = m_email;
		this.m_addr = m_addr;
	}
	
	

	public String getM_addr() {
		return m_addr;
	}
	public void setM_addr(String m_addr) {
		this.m_addr = m_addr;
	}
	public int getM_code() {
		return m_code;
	}
	public void setM_code(int m_code) {
		this.m_code = m_code;
	}
	public String getM_id() {
		return m_id;
	}
	public void setM_id(String m_id) {
		this.m_id = m_id;
	}
	public String getM_pass() {
		return m_pass;
	}
	public void setM_pass(String m_pass) {
		this.m_pass = m_pass;
	}
	public String getM_name() {
		return m_name;
	}
	public void setM_name(String m_name) {
		this.m_name = m_name;
	}
	public String getM_email() {
		return m_email;
	}
	public void setM_email(String m_email) {
		this.m_email = m_email;
	}
	public String getM_api() {
		return m_api;
	}
	public void setM_api(String m_api) {
		this.m_api = m_api;
	}
	public String getM_emailcheck() {
		return m_emailcheck;
	}
	public void setM_emailcheck(String m_emailcheck) {
		this.m_emailcheck = m_emailcheck;
	}
	
	
	
}
