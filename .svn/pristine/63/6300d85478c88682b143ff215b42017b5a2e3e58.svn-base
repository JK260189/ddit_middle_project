/* 메뉴 버튼*/
#menuBtn {
	position: fixed;
	top: 175px;
	left: 10;
	z-index: 20;
	opacity: 0.8;
}

input[id="menuIcon"] {
	display: none;
}

input[id="menuIcon"] + label {
	display: block;
	width: 50px;
	height: 30px;
	position: relative;
	cursor: pointer;
}

input[id="menuIcon"] + label span {
	display: block;
	position: absolute;
	width: 100%;
	height: 5px;
	border-radius: 30px;
	background: #5A5A5A;
	border: 1px solid white;
	transition: all .35s;
}

input[id="menuIcon"] + label span:nth-child(1){
	top:0;
}

input[id="menuIcon"] + label span:nth-child(2){
	top:50%;
	transform:translateY(-50%);
}

input[id="menuIcon"] + label span:nth-child(3){
	bottom:0;
}

input[id="menuIcon"]:checked + label {
	z-index: 20;
}


input[id="menuIcon"]:checked + label span:nth-child(1){
	top:50%;
	transform:translateY(-50%) rotate(-45deg);
}

input[id="menuIcon"]:checked + label span:nth-child(2){
	opacity:0;
}

input[id="menuIcon"]:checked + label span:nth-child(3){
	bottom:50%;
	transform:translateY(50%) rotate(45deg);
}




/* 메뉴 바 */
div.flex-shrink-0 {
	visibility: hidden;
	width: 280px;
	height: 100%;
	
	position: fixed;
	top: 175px;
	left: -280px;
	z-index: 10;
	transition:all .35s;
	opacity: 0.8;
}


input[id="menuIcon"]:checked + label + div {
	visibility: visible;
	width: 280px;
	height: 100%;
	
	position: fixed;
	top: 175px;
	left: 0px;
	z-index: 10;
	transition:all .35s;
	opacity: 0.8;
}








