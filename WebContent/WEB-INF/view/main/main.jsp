<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<main>

<%-- 홈페이지 첫 화면 --%>
	<div id="myCarousel" class="carousel slide" data-bs-ride="carousel">
		<div class="carousel-indicators">
			<button type="button" data-bs-target="#myCarousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
			<button type="button" data-bs-target="#myCarousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
			<button type="button" data-bs-target="#myCarousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
		</div>
	
	
		<div class="carousel-inner">
		
			<div class="carousel-item active" onclick="">
				<img src="<%=request.getContextPath()%>/images/jeju.jpg" class="d-block w-100 h-100" alt="제주도">
				<div class="container">
					<div class="carousel-caption text-start">
						<h1>천혜의 자연 제주도</h1>
						<p>세계자연유산 제주도로...</p>
					</div>
				</div>
			</div>


			<div class="carousel-item" onclick="">
				<img src="<%=request.getContextPath()%>/images/kyungju.jpg" class="d-block w-100 h-100" alt="경주">
				<div class="container">
					<div class="carousel-caption">
						<h1>천년 고도 경주</h1>
						<p>세계 유산 </p>
					</div>
				</div>
			</div>
	
			<div class="carousel-item" onclick="">
				<img src="<%=request.getContextPath()%>/images/Ulleung.jpg" class="d-block w-100 h-100" alt="울릉도">
				<div class="container">
					<div class="carousel-caption text-end">
						<h1>울릉도</h1>
						<p>아름다운 신비의 섬</p>
					</div>
				</div>
			</div>
		</div>


		<!-- 배너 슬라이드 버튼 -->
		<button class="carousel-control-prev" type="button" data-bs-target="#myCarousel" data-bs-slide="prev">
			<span class="carousel-control-prev-icon" aria-hidden="true"></span>
			<span class="visually-hidden">Previous</span>
		</button>

		<button class="carousel-control-next" type="button" data-bs-target="#myCarousel" data-bs-slide="next">
			<span class="carousel-control-next-icon" aria-hidden="true"></span>
			<span class="visually-hidden">Next</span>
		</button>
	</div><!-- 		+++		배너 종료	+++		 -->



	<!-- 배너 밑 본문 -->
	<!--	+++		상품 동그라미 전시 		+++		-->
	<div class="container marketing">

<!-- Three columns of text below the carousel -->
		<div class="row">
			<!-- 	상품 1 		 -->
			<div class="col-lg-4">
				<div class="onImage">
				<img src="<%=request.getContextPath()%>/images/jeju2.jpg" class="bd-placeholder-img rounded-circle" width="300" height="300" alt="제주도">
				<p class="col-lg-4_text" >제주도로</p>
				</div>
			

			</div><!-- /.col-lg-4 -->
		
		<!-- 	상품 2		 -->
		<div class="col-lg-4">
			<div class="onImage">
			<img src="<%=request.getContextPath()%>/images/kangwondo.jpg" class="bd-placeholder-img rounded-circle" width="300" height="300" alt="인제">
			<p class="col-lg-4_text">인제 자작나무숲</p>
			</div>

		</div><!-- /.col-lg-4 -->


		<div class="col-lg-4">
			<div class="onImage">
			<img src="<%=request.getContextPath()%>/images/andong.jpg" class="bd-placeholder-img rounded-circle" width="300" height="300" alt="안동">
			<p class="col-lg-4_text">안동 월정교</p>
			</div>


		</div><!-- /.col-lg-4 -->
	</div><!-- /.row --> <!--	+++		상품 동그라미 전시 종료 		+++		-->


<!-- START THE FEATURETTES -->

	<hr>

	


	<hr>


	


	<hr>


	


	<hr>

<!-- /END THE FEATURETTES -->

</div><!-- /.container -->



</main>